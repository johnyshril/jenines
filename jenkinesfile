pipeline{
   agent any
   tools{
      maven 'Maven'
    }
    stages{'Build Maven'}{
      
      steps{
         checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/johnyshril/jenines.git']])
         sh 'mvn clean install'
         
      }
    }
   
}
